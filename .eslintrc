{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {}
	},
	"rules": {
		"constructor-super": 2,
		"for-direction": 2,
		"getter-return": 2,
		"no-case-declarations": 2,
		"no-class-assign": 2,
		"no-compare-neg-zero": 2,
		"no-cond-assign": 2,
		"no-control-regex": 2,
		"no-console": 0,
		"no-debugger": 2,
		"no-delete-var": 2,
		"no-dupe-args": 2,
		"no-dupe-class-members": 2,
		"no-dupe-keys": 2,
		"no-duplicate-case": 2,
		"no-empty-character-class": 2,
		"no-empty-pattern": 2,
		"no-ex-assign": 2,
		"no-extra-boolean-cast": 2,
		"no-extra-semi": 2,
		"no-fallthrough": 2,
		"no-global-assign": 2,
		"no-inner-declarations": 2,
		"no-irregular-whitespace": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-new-symbol": 2,
		"no-obj-calls": 2,
		"no-octal": 2,
		"no-redeclare": 2,
		"no-regex-spaces": 2,
		"no-self-assign": 2,
		"no-sparse-arrays": 2,
		"no-this-before-super": 2,
		"no-unexpected-multiline": 2,
		"no-unreachable": 2,
		"no-unsafe-finally": 2,
		"no-unsafe-negation": 2,
		"no-unused-labels": 2,
		"no-unused-vars": 2,
		"no-useless-escape": 2,
		"require-yield": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"array-bracket-spacing": 2,
		"array-element-newline": 2,
		"arrow-spacing": 2,
		"brace-style": 2,
		"block-spacing": 2,
		"camelcase": 2,
		"default-case": 2,
		"eqeqeq": 2,
		"func-call-spacing": 2,
		"func-name-matching": 2,
		"func-names": 0,
		"func-style": 0,
		"function-paren-newline": 2,
		"generator-star-spacing": 2,
		"global-require": 2,
		"keyword-spacing": 2,
		"no-alert": 2
	},
	"env": {
		"browser": true,
		"node": true,
		"worker": true,
		"mocha": true,
		"jasmine": true,
		"es6": true
	}
}
